			</basic>
			<basic name="fullname" />
			<basic name="gender" />
			<basic name="birthday">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="email" />
			<basic name="address" />
			<basic name="blog" />
			<basic name="mark" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<entity class="com.liferay.tutorial.model.impl.Validate_StudentImpl" name="Validate_Student">
		<table name="Validate_Student" />
		<attributes />
	</entity>
</entity-mappings>